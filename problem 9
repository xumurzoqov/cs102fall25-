#include <iostream>
using namespace std;

int main() {
    int x;
    cout << "Enter a number: ";
    cin >> x;

    if (x < 20) {
        cout << "Too slow" << endl;
    } else if (x > 80) {
        cout << "Too fast" << endl;
    } else {
        cout << "Just right" << endl;
    }

    return 0;
}
