#include <iostream>
using namespace std;

int main() {
    cout << "Enter a character: ";
    char ch;
    cin >> ch;

    if (ch >= 65 && ch <= 90)
        cout << ch << " is uppercase";
    else if (ch >= 97 && ch <= 122)
        cout << ch << " is lowercase";
    else
        cout << ch << " is not in the alphabet";

    return 0;
}
