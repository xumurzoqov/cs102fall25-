#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
int main() {
double a, b, c;
    cout << "Enter coefficients a, b, and c";
    cin >> a >> b >>c;
    if (a==0) {
        cout << " this is not quadratic equation" << endl;
        return 1;
    }
    double D=(b*b)-4*a*c;
    cout << fixed << setprecision(3);
    if (D>0) {
         double x1=(-b+sqrt (D))/(2*a);
        double x2=(-b-sqrt (D))/(2*a);
        cout<< "x1=" <<x1<<endl;
        cout<< "x2="<<x2<<endl;
    }else if (D==0) {
        double x1= -b/(2*a);
        cout<< "x1=x2"<<x1<<endl;
    }else {
        cout<<"not real roots"<<endl;
    }
    return 0;
}
